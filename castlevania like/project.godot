config_version=5

[application]

config/name="Castlevania like"
run/main_scene="res://scenes/levels/start_level/level_outside.tscn"
config/features=PackedStringArray("4.2", "Mobile")
config/icon="res://icon.svg"

[audio]

manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"
manager/sound/pool_3D_size=16

[autoload]

SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
PlayerGlobal="*res://scenes/player/player_decomposed/player_global.tscn"
PhantomCameraManager="*res://addons/phantom_camera/scripts/managers/phantom_camera_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"
CollisionSoundManager="*res://scenes/SoundManager/collision_sound_manager.tscn"
GlobalSignals="*res://scenes/Global Signals/global_signals.gd"

[display]

window/size/viewport_width=256
window/size/viewport_height=180
window/size/window_width_override=1022
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/AS2P/plugin.cfg", "res://addons/godot_state_charts/plugin.cfg", "res://addons/phantom_camera/plugin.cfg", "res://addons/resonate/plugin.cfg")

